<main class="con-main-halfway">
    <div id="radio_cont">
        <div id="radio_flax">
            <label class="radio-label"> <input type="radio" name="option" value="convenience_store" />편의점 </label>
            <label class="radio-label"> <input type="radio" name="option" value="cafe" />카페 </label>
            <label class="radio-label"> <input type="radio" name="option" value="subway_station" />지하철 </label>
            <label class="radio-label"> <input type="radio" name="option" value="bus_station" />버스 </label>
            <label class="radio-label"> <input type="radio" name="option" value="restaurant" />음식점 </label>
            <label class="radio-label"> <input type="radio" name="option" value="library" />도서관 </label>
        </div>
    </div>
    <div>
        <header class="search-bar">
            <div class="con-search">
                <img class="searchIcon" src="searchIcon.svg" alt="" />
                <input class="search-input post_input_data" autocomplete="none" type="text" id="textInput" class="form-control" name="address" placeholder="지점 1" />
                <img id="reset-button" class="xMark xMark_event" src="xMark.svg" alt="지우기" />
            </div>
            <button id="mid_btn" class="search-btn" type="button">찾기</button>
        </header>
        <div class="search-bar">
            <div class="con-search way">
                <img class="searchIcon" src="searchIcon.svg" alt="" />
                <input class="search-input post_input_data" autocomplete="none" type="text" id="textInput" class="form-control" name="address" placeholder="지점 2" />
                <img id="reset-button" class="xMark xMark_event" src="xMark.svg" alt="지우기" />
            </div>
            <button id="" class="remove-btn remove-address" type="button">삭제</button>
        </div>
        <div class="way-list"></div>
        <button class="add-address" type="button">+</button>
    </div>
</main>
<script src="Halfway.js"></script>
